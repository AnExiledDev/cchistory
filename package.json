{
	"name": "@mariozechner/cchistory",
	"version": "1.1.8",
	"description": "Extract and compare prompts from different Claude Code versions",
	"main": "dist/index.js",
	"bin": {
		"cchistory": "dist/cchistory.js"
	},
	"scripts": {
		"build": "tsc && chmod +x dist/cchistory.js",
		"dev": "tsc --watch --preserveWatchOutput",
		"clean": "rm -rf dist",
		"prepublishOnly": "npm run clean && npm run build",
		"check": "biome check --write . && tsc --noEmit",
		"prepare": "husky"
	},
	"files": [
		"dist/**/*",
		"README.md"
	],
	"keywords": [
		"claude",
		"anthropic",
		"prompts",
		"history",
		"analysis"
	],
	"author": "Mario Zechner",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/badlogic/cchistory.git"
	},
	"engines": {
		"node": ">=16.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.1.4",
		"@types/node": "^22.10.5",
		"husky": "^9.1.7",
		"tsx": "^4.20.3",
		"typescript": "^5.9.2"
	},
	"types": "./dist/index.d.ts",
	"dependencies": {
		"chalk": "^5.5.0"
	}
}
